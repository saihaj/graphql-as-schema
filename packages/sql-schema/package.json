{
  "name": "@saihaj/graphql-to-sqlite",
  "version": "0.0.0",
  "description": "Generate SQL schema from GraphQL schema",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "repository": {
    "type": "git",
    "url": "https://github.com/saihaj/graphql-as-schem",
    "directory": "packages/sql-schema"
  },
  "bin": {
    "graphql-to-sqlite": "dist/bin.js"
  },
  "scripts": {
    "build": "tsc",
    "start": "ts-node ./src/index.ts",
    "prepack": "bob prepack",
    "check": "tsc --pretty --noEmit"
  },
  "buildOptions": {
    "input": "./src/index.ts",
    "bin": {
      "graphql-to-sqlite": {
        "input": "src/bin.ts"
      }
    }
  },
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./*": {
      "require": "./dist/*.js",
      "import": "./dist/*.mjs"
    }
  },
  "typings": "dist/index.d.ts",
  "typescript": {
    "definition": "dist/index.d.ts"
  },
  "publishConfig": {
    "directory": "dist",
    "access": "public"
  },
  "author": "Saihajpreet Singh <saihajpreet.singh@gmail.com> (https://saihaj.dev/)",
  "license": "MIT",
  "dependencies": {
    "@graphql-tools/graphql-file-loader": "^7.3.7",
    "@graphql-tools/load": "^7.5.5",
    "commander": "^9.1.0",
    "graphql": "^16.3.0"
  },
  "devDependencies": {
    "@types/node": "^17.0.21",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.2"
  }
}
